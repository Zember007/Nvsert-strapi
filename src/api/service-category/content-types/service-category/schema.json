{
  "kind": "collectionType",
  "collectionName": "service_categories",
  "info": {
    "singularName": "service-category",
    "pluralName": "service-categories",
    "displayName": "Service Category",
    "description": "Groups of services"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "category"
    }
  }
}


