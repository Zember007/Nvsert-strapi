{
  "kind": "collectionType",
  "collectionName": "feedback_categories",
  "info": {
    "singularName": "feedback-category",
    "pluralName": "feedback-categories",
    "displayName": "Feedback Category",
    "description": "Categories for organizing feedbacks"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": false
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "feedbacks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::feedback.feedback",
      "mappedBy": "category"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
